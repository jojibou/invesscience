   "cell_type": "code",
   "execution_count": 215,
   "metadata": {},
   "outputs": [],
   "source": [
    "def get_people_top(degrees, people):\n",
    "    ''' get a new column is_top50? for each employee in the people table'''\n",
    "    degrees.institution = degrees.institution.fillna('No info')\n",
    "    degrees['degree_standardization'] = degrees.institution.map(lambda x: degree_standardisation(x.lower()))\n",
    "    degrees['is_top50'] = degrees.degree_standardization.map(lambda x: 0 if x=='no top' else 1)\n",
    "    is_top_df = pd.DataFrame(degrees.groupby('object_id')['is_top50'].max()).reset_index().rename(columns = {'object_id': 'person_object_id'})\n",
    "    merge = people.merge(is_top_df, how = 'left', on ='person_object_id')\n",
    "    return merge"
   ]
